{
  "manifest_version": 2,
  "name": "Free Music",
  "description": "The extension helps users who used to listen to music on Chinese websites(e.g. music.163.com, xiami.com) but far away from china.",
  "version": "3.0",
  "minimum_chrome_version": "23.0",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "proxy",
    "http://cn-proxy.com/"
  ],
  "default_locale": "en"
}
